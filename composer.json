{
    "name": "carbon/plausible",
    "type": "neos-carbon",
    "description": "Track visits of your Neos site with plausible.io",
    "license": "GPL-3.0",
    "keywords": [
        "flow",
        "neos",
        "analytics",
        "plausible",
        "carbon"
    ],
    "require": {
        "neos/neos": "^7.3 || ^8.0",
        "neos/cache": "^7.3 || ^8.0",
        "neos/fusion-afx": "*",
        "carbon/eel": "^2.7",
        "tedivm/jshrink": "^1.4"
    },
    "suggest": {
        "neos/seo": "SEO configuration and tools for Neos."
    },
    "autoload": {
        "psr-4": {
            "Carbon\\Plausible\\": "Classes"
        }
    },
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "hello@uhlmann.pro",
            "homepage": "https://www.uhlmann.pro",
            "role": "Developer"
        }
    ],
    "archive": {
        "exclude": [
            ".editorconfig",
            ".gitattributes",
            ".github",
            ".gitignore",
            ".nvmrc",
            ".prettierrc",
            "CODE_OF_CONDUCT.md",
            "LICENSE",
            "package.json",
            "yarn.lock"
        ]
    }
}
