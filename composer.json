{
    "name": "carbon/plausible",
    "type": "neos-carbon",
    "description": "Track visits of your Neos site with plausible.io",
    "license": "GPL-3.0",
    "keywords": ["flow", "neos", "analytics", "plausible", "carbon"],
    "require": {
        "neos/neos": "^5.0 || ^7.0",
        "neos/fusion-afx": "*",
        "tedivm/jshrink": "^1.4"
    },
    "suggest": {
        "neos/seo": "SEO configuration and tools for Neos."
    },
    "autoload": {
        "psr-4": {
            "Carbon\\Plausible\\": "Classes"
        }
    },
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch",
            "homepage": "https://jonnitto.ch",
            "role": "Developer"
        }
    ],
    "archive": {
        "exclude": [
            ".editorconfig",
            ".gitattributes",
            ".github",
            ".gitignore",
            ".prettierrc",
            "CODE_OF_CONDUCT.md",
            "LICENSE"
        ]
    }
}
